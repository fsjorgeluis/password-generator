		"url": "https://fsjorgeluis.github.io/portfolio"
	},
	"contributors": [
		{
			"name": "Javier Mora",
			"email": "javmora04@gmail.com",
			"url": "https://j4viermora.github.io/hola/"
		}
	],
	"license": "ISC",
	"devDependencies": {
		"@types/jest": "^26.0.24",
		"jest": "^27.0.6",
		"ts-jest": "^27.0.3",
		"typescript": "^4.3.5"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "./test",
